<div class="modal fade" id="cancelRequestModal" tabindex="-1" role="dialog" aria-labelledby="cancelRequestModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
        <h4 class="modal-title" id="addPetModal">Cancel Request</h4>
      </div>
      <div class="modal-body">
        <h4 class="text-center">Are you sure you want to delete this request?</h4>
          <%= button_to request_path(request_id), {method: :delete, class:"btn btn-default text-center", remote: true} do  %>
            Yes
          <% end %>
          <!-- <a>
            <a class="btn btn-default text-center" data-dismiss="modal">No</a>
          </a> -->
        <!-- <p><%# reqObj %></p> -->
        <%# link_to "Cancel Request", delete_request_path, method: 'post', id: "cancel-request-btn"%>
        <!-- <a class="cancel-request-btn" data-request-id=<%# request.id %>>Cancel Request</a> -->
      </div>
    </div>
  </div>
</div>
