<nav class="navbar">
  <div class="container-fluid">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
      <% if user_signed_in? %>
      <a class="navbar-brand" href="/"><%= current_user.first_name%> </a>
      <% end %>
    </div>
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">


      <ul class="nav navbar-nav">
        <% if user_signed_in? %>
        <li class="active"><%= link_to "Community", users_path%><span class="sr-only">(current)</span></li>
        <li class="active"><%= link_to "Requests", requests_path%><span class="sr-only">(current)</span></li>
          <li class="active"><%= link_to "Logout", destroy_user_session_path, method: :delete %><span class="sr-only">(current)</span></li>
        <% else %>
          <li class="active"><%= link_to "Sign up", new_user_registration_path %><span class="sr-only">(current)</span></li>
          <li class="active"><%= link_to "Sign in with Facebook", user_omniauth_authorize_path(:facebook) %><span class="sr-only">(current)</span></li>
          <li class="active"><%= link_to "Login", new_user_session_path %><span class="sr-only">(current)</span></li>
        <% end %>

        <% if notice %>
          <p class="alert alert-success"><%= notice %></p>
        <% end %>
        <% if alert %>
          <p class="alert alert-danger"><%= alert %></p>
        <% end %>
      </ul>
    </div><!-- /.navbar-collapse -->
  </div><!-- /.container-fluid -->
